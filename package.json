  "scripts": {
    "build:clean": "rm -rf dist/ && rm -f color-picker-1.0.0-*.*.*.tgz",
    "start": "react-scripts start",
    "lint:ts": "eslint src --ext .ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "lint": "npm run lint:css & npm run lint:ts",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts,.tsx",
    "build:css": "cp src/Components/*.css dist/Components && cp src/*.css dist && cp src/Components/Options/*.css dist/Components/Options",
    "build:dist": "npm run build:ts && npm run build:babel && npm run build:css",
    "ci:build": "npm run build:clean &&  npm run build:ts &&  npm run build:babel && npm run build:css",
